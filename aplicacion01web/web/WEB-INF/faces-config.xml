<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/resourses/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
 <from-view-id>*</from-view-id>
 
  <navigation-case>
 <from-outcome>temp</from-outcome>
 <to-view-id>/template.xhtml</to-view-id>
 </navigation-case>
 
 <navigation-case>
 <from-outcome>inicio</from-outcome>
 <to-view-id>/index.xhtml</to-view-id>
 </navigation-case>
 
 <navigation-case>
 <from-outcome>asignar_cita</from-outcome>
 <to-view-id>/citas/Create.xhtml</to-view-id>
 </navigation-case>
 
 <navigation-case>
 <from-outcome>consultar_citas</from-outcome>
 <to-view-id>/citas/List.xhtml</to-view-id>
 
 </navigation-case>
 
  <navigation-case>
 <from-outcome>consultar_consultorios</from-outcome>
 <to-view-id>/consultorios/List.xhtml</to-view-id>
 
 </navigation-case>
  <navigation-case>
 <from-outcome>registrar_consultorios</from-outcome>
 <to-view-id>/consultorios/Create.xhtml</to-view-id>
 
 </navigation-case>
 
  <navigation-case>
 <from-outcome>consultar_medicos</from-outcome>
 <to-view-id>/medicos/List.xhtml</to-view-id>
 
 </navigation-case>
 
   <navigation-case>
 <from-outcome>registrar_medicos</from-outcome>
 <to-view-id>/medicos/Create.xhtml</to-view-id>
 
 </navigation-case>
 
    <navigation-case>
 <from-outcome>registrar_pacientes</from-outcome>
 <to-view-id>/pacientes/Create.xhtml</to-view-id>
 
 </navigation-case>
 
   <navigation-case>
 <from-outcome>consultar_pacientes</from-outcome>
 <to-view-id>/pacientes/List.xhtml</to-view-id>
 
 </navigation-case>
 </navigation-rule>
</faces-config>

